{
  "name": "yumemi-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .tsx,.ts src/",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
